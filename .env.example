# Azure Cosmos DB Configuration
# Copy this file to .env and fill in your actual values

# Cosmos DB endpoint (from Azure Portal)
COSMOS_DB_ENDPOINT=https://your-account.documents.azure.com:443/

# Cosmos DB key (from Azure Portal - Primary or Secondary Key)
COSMOS_DB_KEY=your-cosmos-db-key-here

# Optional: Database ID (defaults to "SeedExchange" if not specified)
COSMOS_DB_DATABASE_ID=SeedExchange

# Optional: Container ID (defaults to "SeedExchanges" if not specified)
COSMOS_DB_CONTAINER_ID=SeedExchanges
